<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>
<h1>Log In!</h1>
<%=form_tag ({controller:'sessions',action:'create',method:'post'}) do%>
<p>Username:<input type="text"name="username"></p>
<p>Password:<input type="password"name="password"></p>
<input type="submit"value="Sign up">

<%end%>
